{
  "name": "@dareksob/stylekit",
  "version": "1.0.4",
  "description": "Collection of utilites for sass",
  "repository": {
    "type": "git",
    "url": "git@github.com:deepblue-networks/stylekit.git"
  },
  "keywords": [
    "scss",
    "tools",
    "utilities"
  ],
  "author": "Darius Sobczak<darius.sobczak@db-n.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vuepress dev",
    "lint": "prettier -l \"src/**/*[.js|.vue|.scss]\"",
    "lint:fix": "prettier --write \"src/**/*[.js|.vue|.scss]\"",
    "test": "sass specs/all.specs.scss -I ./node_modules -I ./src",
    "test:w": "nodemon --exec \"npm test\" --watch specs",
    "test:general": "sass tests/general.scss tests/general.css",
    "test:general:w": "sass tests/general.scss tests/general.css -w",
    "build-examples": "sass examples:examples",
    "deploy": "npm publish --access public"
  },
  "devDependencies": {
    "foundation-sites": "^6.5.0",
    "sass": "^1.0.0",
    "nodemon": "^1.17.5",
    "sass-true": "^6.1.0",
    "vuepress": "^1.8.1"
  }
}
